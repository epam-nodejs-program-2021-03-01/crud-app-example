{
  "name": "crud-app-example",
  "version": "0.0.1",
  "description": "Example of a CRUD Node.JS Express server",
  "license": "MIT",
  "private": true,
  "repository": {
    "type": "git",
    "url": "git+https://github.com/epam-nodejs-program-2021-03-01/crud-app-example.git"
  },
  "bugs": {
    "url": "https://github.com/epam-nodejs-program-2021-03-01/crud-app-example/issues"
  },
  "homepage": "https://github.com/epam-nodejs-program-2021-03-01/crud-app-example#readme",
  "engines": {
    "node": "14.x"
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "clean": "del-cli dist/*",
    "build:only": "tsc --project ./tsconfig.production.json",
    "compile": "npm run build:only -- --noEmit",
    "prebuild": "npm run clean",
    "build": "npm run build:only",
    "postbuild": "del-cli dist/**/*.type.js* dist/**/typings/**/*.js*",
    "lint": "eslint --ext .js,.ts src",
    "start": "node --require source-map-support/register --require ./env/remote.js dist/index.js",
    "dev": "nodemon --ext json,ts --require ./env/local.ts src/index.ts",
    "preplay": "touch src/playground.ts",
    "play": "ts-node --require ./env/local.ts src/playground.ts"
  },
  "devDependencies": {
    "@types/express": "4.17.11",
    "@types/express-request-id": "1.4.1",
    "@types/jsonwebtoken": "8.5.1",
    "@types/morgan": "1.9.2",
    "@types/ms": "0.7.31",
    "@types/pg": "7.14.11",
    "@types/promise-timeout": "1.3.0",
    "@types/uuid": "8.3.0",
    "@typescript-eslint/eslint-plugin": "4.22.0",
    "@typescript-eslint/parser": "4.22.0",
    "del-cli": "3.0.1",
    "eslint": "7.24.0",
    "nodemon": "2.0.7",
    "ts-node": "9.1.1",
    "typescript": "4.2.4"
  },
  "dependencies": {
    "celebrate": "14.0.0",
    "dotenv-extended": "2.9.0",
    "express": "5.0.0-alpha.8",
    "express-allow-methods": "0.2.2",
    "express-request-id": "1.4.1",
    "joi": "17.4.0",
    "jsonwebtoken": "8.5.1",
    "morgan": "1.10.0",
    "ms": "2.1.3",
    "pg": "8.6.0",
    "pg-hstore": "2.3.3",
    "promise-timeout": "1.3.0",
    "sequelize": "6.6.2",
    "source-map-support": "0.5.19",
    "winston": "3.3.3",
    "winston-console-format": "1.0.8"
  }
}
